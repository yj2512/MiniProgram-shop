/* pages/myInfo-address/myInfo-address.wxss */
.container{
  background-color: #f5f5f5;
  min-height: 1200rpx;
  padding-bottom: 200rpx
}
.section-ft{
  width: 100%;
  height: 80rpx;
  background-color: #de2022;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #ffffff;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 3
}
.address-item{
  background-color: #ffffff;
  height: 198rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  padding-left: 26rpx;
  padding-right: 26rpx
}
.contact{
  display: flex;
  justify-content: space-between;
  line-height: 50rpx;
  margin-top: 10rpx
}
.address{
  line-height: 50rpx;
  margin-bottom: 10rpx
}
.item-ft{
  height: 74rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx #f6f6f6 solid
}
.item-ft-left{
  display: flex;
  align-items: center
}
.item-ft-left icon{
  margin-right: 22rpx
}
.item-ft-right{
  display: flex;
  line-height: 74rpx
}
.item-ft-right image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 15rpx
}
.item-ft-right text{
  line-height: 74rpx
}
.edit{
  margin-right: 50rpx
}
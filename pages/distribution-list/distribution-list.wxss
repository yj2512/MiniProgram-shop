/* pages/distribution-list/distribution-list.wxss */
.container{
  background-color: #f2f2f2;
  min-height: 1200rpx;
}
.title-list{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
  background-color: #ffffff;
  text-align: center;
  display: flex;
  justify-content: space-around
}
.section-ft{
  font-size: 24rpx;
  line-height: 76rpx;
  text-align: center;
  color: #878787;
}
.distribution-item{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  background-color: #ffffff;
  margin-top: 2rpx;
  text-align: center;
  display: flex;
  justify-content: space-around
}
.memberName,.title-item:nth-of-type(1){
  width: 120rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
.time,.title-item:nth-of-type(2){
  width: 260rpx;
}
.money,.rebate,.title-item:nth-of-type(3),.title-item:nth-of-type(4){
  width: 140rpx;
}
.money,.rebate{
  color: #fe0000
}
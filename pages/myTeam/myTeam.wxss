/* pages/myTeam/myTeam.wxss */
.container{
  background-color: #f2f2f2;
  min-height: 1200rpx;
}
.section-ft{
  font-size: 24rpx;
  line-height: 76rpx;
  text-align: center;
  color: #878787;
}
.myTeam-item{
  height: 200rpx;
  background-color: #ffffff;
  border-top: 2rpx #f1f1f1 solid;
  font-size: 26rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center
}
.memberInfo{
  width: 210rpx;
  height: 120rpx;
  display: flex;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
.memberPic{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.memberLevel,.joinTime{
  font-size: 24rpx;
  color: #9a9a9a
}
.teamInfo{
  height: 120rpx;
  text-align: right
}

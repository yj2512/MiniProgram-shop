.container{
  background-color: #f5f5f5;
  min-height: 1200rpx
}
.form-item{
  height: 88rpx;
  border-bottom: 2rpx #f3f3f3 solid;
  display: flex;
  font-size: 28rpx;
  line-height: 88rpx;
  padding-left: 26rpx;
  background-color: #ffffff
}
.form-item label{
  width: 175rpx;
  height: 88rpx;
}
.form-item input{
  width: 480rpx;
  height: 88rpx
}
.form-textarea textarea{
  background-color: #ffffff;
  width: 100%;
  height: 160rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  padding-left: 35rpx;
  line-height: 80rpx;
}
.textareaStyle{
  line-height: 80rpx
}
.switch{
  font-size: 28rpx;
  padding-left: 26rpx;
  padding-right: 35rpx;
  background-color: #ffffff;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-between
}
.section-ft{
  width: 100%;
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffffff;
  background-color: #e64240;
  position: fixed;
  left: 0;
  bottom: 0
}